{
  "root": "./docs",
  "title": "xxxAPI 文档",
  "description": "xxx API文档的描述",
  "author": "abingoal",
  "language": "zh-hans",
  "plugins": [
    "anchorjs",
    "splitter",
    "tbfed-pagefooter",
    "toggle-chapters",
    "favicon",
    "alerts",
    "versions-select",
    "-sharing"
  ],
  "pluginsConfig": {
    "anchorjs": {
      "icon": "♠",
      "placement": "left",
      "visible": "always"
    },
    "tbfed-pagefooter": {
      "copyright": "Copyright &copy xxx公司 2018",
      "modify_label": "文件修订时间：",
      "modify_format": "YYYY-MM-DD HH:mm:ss"
    },
    "favicon": {
      "shortcut": "assets/favicon.ico",
      "bookmark": "assets/favicon.ico",
      "appleTouch": "assets/apple-touch-icon.png"
    },
    "versions": {
      "options": [
        {
          "text": "v1.0.0",
          "value": ""
        },
        {
          "text": "v2.0.0",
          "value": ""
        }
      ]
    },
    "theme-default": {
      "showLevel": true,
      "styles": {
        "website": "./styles/style.css"
      }
    }
  }
}
